   });
   product_title.click(function(){
     jQuery('html, body').animate({scrollTop: 0}, 700);
   });
   jQuery('#footer_top').click(function(){
     jQuery('html, body').animate({scrollTop: 0}, 700);
   })
 });

 function reset_product_title_top(product_title, product_title_top){
   var document_scroll_top = jQuery(document).scrollTop();
   if (document_scroll_top > product_title_top) {
     product_title.css('top', document_scroll_top);
   }
   if (document_scroll_top <= product_title_top) {
     product_title.css('top', product_title_top);
   }
 }
